<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <bean id="hydraService" class="com.jd.bdp.startUp.collector.simulation.support.TestHydraService">
    </bean>
    <dubbo:registry file="~/.dubbo/hydraTest" />
    <dubbo:protocol name="dubbo" port="20996" />
    <dubbo:service interface="com.jd.bdp.hydra.dubbomonitor.HydraService" ref="hydraService" filter="-hydra" >
    </dubbo:service>

</beans>