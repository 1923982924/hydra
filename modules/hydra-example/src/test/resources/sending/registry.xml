<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:protocol name="dubbo" port="20995" />
    <!--LeaderService-开机-->
    <dubbo:service interface="com.jd.bdp.hydra.dubbomonitor.LeaderService" ref="leadService" filter="-hydra">
    </dubbo:service>
    <bean id="leadService" class="com.jd.bdp.sending.TestLeaderService"></bean>

    <!-- HydraService发作诉讼 -->
    <dubbo:service interface="com.jd.bdp.hydra.dubbomonitor.HydraService" ref="hydraService" filter="-hydra" >
    </dubbo:service>
    <bean id="hydraService" class="com.jd.bdp.sending.TestHydraService"></bean>

</beans>