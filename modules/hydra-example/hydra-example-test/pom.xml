    <name>HYDRA EXAMPLE TEST</name>
    <profiles>
        <!-- 48-zk-test1-->
        <profile>
            <id>zk-48</id>
            <activation>
                <!--默认启用的是48环境配置-->
                <activeByDefault>true</activeByDefault>
                <property>
                    <name>my_env</name>
                    <value>test-48</value>
                </property>
            </activation>
            <properties>
                <file_name>zk-48</file_name>
                <dubbo.registry.address>192.168.227.48:2181</dubbo.registry.address>
